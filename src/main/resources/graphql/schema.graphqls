type Query {
  users: [User]
  userByEmail(email: String!): User
  movies: [Movie]
}

type User {
  email: String!
  name: String
  password: String
  wishlists: [Wishlist]
}

type Wishlist {
  id: ID!
  name: String
  movies: [Movie]
}

type Movie {
  id: ID!
  title: String
  releaseYear: Int
  wishlists: [Wishlist]
}
